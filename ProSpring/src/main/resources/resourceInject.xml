<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans	      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="resourceBean1" class="spring.helloworld.resourceloader.ResourceBean3">
        <property name="resource" value="spring/helloworld/test.properties"/>
    </bean>

    <bean id="resourceBean2" class="spring.helloworld.resourceloader.ResourceBean3">
        <property name="resource" value="classpath:spring/helloworld/test.properties"/>
    </bean>
</beans>